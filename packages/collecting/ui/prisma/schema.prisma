// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserPacks {
  username String
  packCode String

  @@id(name: "usernamePackCode", fields: [username, packCode])
  @@index([username])
}
